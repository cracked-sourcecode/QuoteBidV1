  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development PORT=5050 FRONTEND_URL=http://localhost:5050 EMAIL_FROM='QuoteBid <no-reply@quotebid.co>' tsx server/index.ts",
    "dev:mobile": "NODE_ENV=development PORT=5051 FRONTEND_URL=http://192.168.1.121:5051 EMAIL_FROM='QuoteBid <no-reply@quotebid.co>' tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && esbuild apps/wsServer.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && esbuild apps/worker/pricingWorker.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "cleanup:signups": "tsx server/scripts/cleanupIncompleteSignups.ts",
    "test": "jest",
    "test:pricing": "vitest run lib/pricing/__tests__/",
    "debug-pitches": "NODE_ENV=development tsx server/debug-pitches.ts",
    "worker:once": "npx tsx apps/worker/pricingWorker.ts --once",
    "worker:dev": "npx tsx apps/worker/pricingWorker.ts",
    "worker:pricing": "npx tsx apps/worker/pricingWorker.ts",
    "ws": "npx tsx apps/wsServer.ts",
    "email:preview": "email dev --dir emails/templates --port 3001",
    "full-stack": "concurrently \"FRONTEND_URL=http://localhost:5050 EMAIL_FROM='QuoteBid <no-reply@quotebid.co>' npm run dev\" \"npm run ws\" \"npm run worker:dev\"",
    "full-stack:mobile": "concurrently \"FRONTEND_URL=http://192.168.1.121:5051 EMAIL_FROM='QuoteBid <no-reply@quotebid.co>' npm run dev:mobile\" \"npm run ws\" \"npm run worker:dev\"",
    "full-stack:production": "concurrently \"npm start\" \"node dist/wsServer.js\" \"node dist/pricingWorker.js\"",
    "full-stack:simple": "node start-production.cjs",
    "check-services": "npx tsx check-services.ts",
    "postinstall": "npm rebuild sharp --platform=linux --arch=x64 || npm rebuild sharp || echo 'Sharp rebuild failed, using fallback'"
  },
